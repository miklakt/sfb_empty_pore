  keywords  = {Ellipsometry,Mixed electrolyte,Neutron reflectometry,Polymer brush,Specific ion effects,Thermoresponsive polymer},
  month     = {3},
  pages     = {292-304},
  pmid      = {33189318},
  publisher = {Academic Press},
  title     = {Competitive specific ion effects in mixed salt solutions on a thermoresponsive polymer brush},
  volume    = {586},
  year      = {2021}
}

