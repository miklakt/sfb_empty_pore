@article{Birshtein1979,
  abstract        = {The general method of obtaining the partition function and thermodynamic characteristics of polymer chains near an adsorbing surface, simulated by random walks on a lattice, is developed. The method takes into account the effect of short-range interactions in polymer chains, in particular, the chain stiffness and secondary structure. The theory of adsorption of chains of different stiffness is developed, and the process of adsorption which occurs when the external conditions change is shown to be always a second-order phase transition. The critical adsorption energy decreases and the sharpness of transition grows when the chain stiffness increases. A simple model of a chain with "virtual" steps is proposed which simplifies the treatment; the results obtained are in good agreement with exact theories. A general scheme of analysis of adsorption of chains with a given secondary structure is set forth and the analogy between the stiffness of a noncooperative chain and the presence of helical segments in a polypeptide chain is discussed. Copyright {\textcopyright} 1979 John Wiley \& Sons, Inc.},
  author          = {Birshtein, T. M. and Zhulina, E. B. and Skvortsov, A. M.},
  doi             = {10.1002/BIP.1979.360180511},
  issn            = {1097-0282},
  journal         = {Biopolymers},
  mendeley-groups = {Particle_insertion},
  month           = {5},
  number          = {5},
  pages           = {1171--1186},
  publisher       = {John Wiley \& Sons, Ltd},
  title           = {{Adsorption of polypeptides on solid surfaces. I. Effect of chain stiffness}},
  url             = {https://onlinelibrary.wiley.com/doi/full/10.1002/bip.1979.360180511},
  volume          = {18},
  year            = {1979}
}
