   pages = {2170},
   pmid = {10733995},
   publisher = {Biophysical Society},
   title = {Imaging and tracking of single GFP molecules in solution},
   volume = {78},
   url = {https://pmc.ncbi.nlm.nih.gov/articles/PMC1300809/},
   year = {2000},
}

